            "Heddiyoussouf\\SequentialAttributeGuard\\": "src/"
        }
    },
    "authors": [
        {
            "name": "HeddiYoussouf1",
            "email": "heddiyoussouf@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"

    },
    "extra": {
        "laravel": {
            "providers": [
                "Heddiyoussouf\\SequentialAttributeGuard\\SequentialAttributeGuardProvider"
            ]
        }
    }
}
